{
  "name": "@onsetsoftware/svelte-local-storage-store",
  "version": "1.1.0",
  "description": "Layer on top of a Svelte writable store, backing up to local storage across multiple tabs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onsetsoftware/svelte-local-storage-store.git"
  },
  "keywords": [
    "svelte",
    "local",
    "storage",
    "writable",
    "store"
  ],
  "author": "On Set Software Ltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onsetsoftware/svelte-local-storage-store/issues"
  },
  "homepage": "https://github.com/onsetsoftware/svelte-local-storage-store#readme",
  "devDependencies": {
    "svelte": "^3.51.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.0",
    "vitest": "^0.18.1"
  },
  "peerDependencies": {
    "svelte": "^3.51.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/svelte-local-storage-store.umd.js",
  "module": "./dist/svelte-local-storage-store.es.js",
  "exports": {
    ".": {
      "import": "./dist/svelte-local-storage-store.es.js",
      "require": "./dist/svelte-local-storage-store.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts"
}
